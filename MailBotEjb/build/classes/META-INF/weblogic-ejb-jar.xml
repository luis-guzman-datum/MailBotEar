<?xml version="1.0" encoding="UTF-8"?>
<weblogic-ejb-jar xmlns="http://xmlns.oracle.com/weblogic/weblogic-ejb-jar" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd http://xmlns.oracle.com/weblogic/weblogic-ejb-jar http://xmlns.oracle.com/weblogic/weblogic-ejb-jar/1.4/weblogic-ejb-jar.xsd">
    <!--weblogic-version:12.1.3-->
	<weblogic-enterprise-bean>
		<ejb-name>SchedulerServiceImpl</ejb-name>
		<stateless-session-descriptor>
			<pool>
				<max-beans-in-free-pool>1</max-beans-in-free-pool>
				<initial-beans-in-free-pool>1</initial-beans-in-free-pool>
			</pool>
		</stateless-session-descriptor>
	</weblogic-enterprise-bean>
	<weblogic-enterprise-bean>
		<ejb-name>GoogleMdb</ejb-name>
		<message-driven-descriptor>
			<pool>
				<max-beans-in-free-pool>1</max-beans-in-free-pool>
				<initial-beans-in-free-pool>1</initial-beans-in-free-pool>
			</pool>
		</message-driven-descriptor>
	</weblogic-enterprise-bean>
	<weblogic-enterprise-bean>
		<ejb-name>ConfiaMdb</ejb-name>
		<message-driven-descriptor>
			<pool>
				<max-beans-in-free-pool>1</max-beans-in-free-pool>
				<initial-beans-in-free-pool>1</initial-beans-in-free-pool>
			</pool>
		</message-driven-descriptor>
	</weblogic-enterprise-bean>
	<weblogic-enterprise-bean>
		<ejb-name>DefaultMdb</ejb-name>
		<message-driven-descriptor>
			<pool>
				<max-beans-in-free-pool>1</max-beans-in-free-pool>
				<initial-beans-in-free-pool>1</initial-beans-in-free-pool>
			</pool>
		</message-driven-descriptor>
	</weblogic-enterprise-bean>
	<weblogic-enterprise-bean>
		<ejb-name>MicrosoftMdb</ejb-name>
		<message-driven-descriptor>
			<pool>
				<max-beans-in-free-pool>1</max-beans-in-free-pool>
				<initial-beans-in-free-pool>1</initial-beans-in-free-pool>
			</pool>
		</message-driven-descriptor>
	</weblogic-enterprise-bean>
	<weblogic-enterprise-bean>
		<ejb-name>YahooMdb</ejb-name>
		<message-driven-descriptor>
			<pool>
				<max-beans-in-free-pool>1</max-beans-in-free-pool>
				<initial-beans-in-free-pool>1</initial-beans-in-free-pool>
			</pool>
		</message-driven-descriptor>
	</weblogic-enterprise-bean>
	<weblogic-enterprise-bean>
		<ejb-name>MailBoxFacadeLocal</ejb-name>
		<transaction-descriptor>
			<trans-timeout-seconds>120</trans-timeout-seconds>
		</transaction-descriptor>
	</weblogic-enterprise-bean>
</weblogic-ejb-jar>